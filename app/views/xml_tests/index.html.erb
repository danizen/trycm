<h1>Listing xml_tests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Xslt</th>
      <th>Output</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @xml_tests.each do |xml_test| %>
      <tr>
        <td><%= xml_test.name %></td>
        <td><%= xml_test.xslt %></td>
        <td><%= xml_test.output %></td>
        <td><%= link_to 'Show', xml_test %></td>
        <td><%= link_to 'Edit', edit_xml_test_path(xml_test) %></td>
        <td><%= link_to 'Destroy', xml_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Xml test', new_xml_test_path %>
